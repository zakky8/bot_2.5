{
  "name": "telegram-moderation-bot",
  "version": "2.0.0",
  "description": "Advanced Telegram moderation bot with full MissRose feature parity",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "node scripts/seed.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "moderation",
    "grammy"
  ],
  "author": "Your Team",
  "license": "MIT",
  "dependencies": {
    "grammy": "^1.21.1",
    "@grammyjs/conversations": "^1.2.0",
    "@grammyjs/storage-redis": "^2.4.2",
    "@grammyjs/i18n": "^1.0.2",
    "@grammyjs/menu": "^1.2.1",
    "@grammyjs/runner": "^2.0.3",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "date-fns": "^3.0.6",
    "axios": "^1.6.5",
    "@openrouter/sdk": "*",
    "ollama": "^0.5.0",
    "ioredis": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1"
  }
}